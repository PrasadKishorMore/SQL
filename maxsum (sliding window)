 int arr[] = {1,3,1,5,6,3,1};

        int k = 3;

       int windowsum = 0;
        for(int i=0; i<k; i++){
            windowsum = windowsum + arr[i];
        }

        int maxsum = windowsum;

        for(int j=k; j<arr.length;j++){
            windowsum = windowsum +arr[j]-arr[j-1];

            maxsum = Math.max(maxsum,windowsum);
        }

        System.err.println(maxsum);
