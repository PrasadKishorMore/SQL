 int arr[] = {4,7,3,9};

        int rotate = 2;

        
        for(int i=0; i<rotate; i++){

            int val = arr[0];

            for(int j=0; j<arr.length-1; j++){

                arr[j] = arr[j+1];
            }

            arr[arr.length-1] = val;
        }

       for(int v : arr){
        System.err.print(v);
       }
